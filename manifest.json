{
  "name": "Spot Play",
  "description": "One-Click Spotify Playlist Player in your Browser.",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/icon.png",
    "128": "assets/icon.png"
